{
    "name": "products",
    "version": "0.0.1",
    "devDependencies": {
        "@sap/ui5-builder-webide-extension": "^1.1.7",
        "@sapui5/ts-types": "^1.90.10",
        "@ui5/cli": "^2.14.1",
        "bestzip": "^2.2.0",
        "karma": "^5.0.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.2",
        "karma-ui5": "^2.1.0",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "build": "npm run clean && ui5 build --include-task=generateManifestBundle generateCachebusterInfo && npm run zip",
        "zip": "cd dist && npx bestzip ../Products-content.zip *",
        "clean": "npx rimraf Products-content.zip dist",
        "test": "npm run karma-ci",
        "karma-ci": "karma start karma-ci.conf.js"
    },
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension"
        ]
    }
}
